    "get-port": "^7.1.0",
    "http-proxy-middleware": "^3.0.3",
    "next": "15.1.3",
    "pm2": "^5.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tiddlywiki": "^5.3.6",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
