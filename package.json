{
  "name": "@tecfancy/mws",
  "version": "0.0.33",
  "bin": {
    "multi-wiki-server": "bin/index.js",
    "mws": "bin/index.js"
  },
  "files": [
    ".next",
    ".env",
    "scripts",
    "package.json"
  ],
  "keywords": [
    "TiddlyWiki",
    "Multiple TiddlyWiki Server",
    "multi-wiki-server",
    "mws"
  ],
  "scripts": {
    "prepare": "next build",
    "dev": "cross-env NODE_ENV=development NAMESPACE=mws node scripts/start.mjs && npm run start:tw",
